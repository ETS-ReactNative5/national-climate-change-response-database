{
  "name": "nccrd",
  "version": "1.0.0",
  "description": "National Climate Change Response Database",
  "scripts": {
    "build": "rimraf ./dist && NODE_ENV=production webpack --config webpack/index.js",
    "start": "rimraf ./dist && onchange \"src/**/*.@(js|jsx|ts|tsx|css|scss)\" -- prettier --write {{changed}} & NODE_ENV=development webpack serve --port 3001 --config webpack/index.js",
    "lint": "eslint -c .eslintrc.cjs \"src/**/*.{js,jsx}\"",
    "prettier": "prettier --write \"./**/*.@(js|jsx|json|mjs|cjs|graphql)\"",
    "ncu": "ncu -x react-router,react-router-dom",
    "ncu -u": "ncu -u -x react-router,react-router-dom"
  },
  "keywords": [],
  "author": "zd.smith@saeon.nrf.ac.za",
  "license": "MIT",
