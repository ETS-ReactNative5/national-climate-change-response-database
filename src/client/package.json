{
  "name": "nccrd",
  "version": "0.0.1",
  "description": "React.js client",
  "scripts": {
    "build": "rimraf ./dist && NODE_ENV=production webpack",
    "start": "rimraf ./dist && onchange \"src/**/*.@(js|jsx|ts|tsx|css|scss)\" -- prettier --write {{changed}} & NODE_ENV=development webpack serve --port 3001 --open",
    "lint": "eslint -c .eslintrc.cjs \"src/**/*.{js,jsx}\"",
    "prettier": "prettier --write \"./**/*.@(js|jsx|json|mjs|cjs|graphql)\"",
    "ncu": "ncu -x @date-io/date-fns",
    "ncu -u": "ncu -u -x @date-io/date-fns"
  },
  "keywords": [],
  "author": "zach@saeon.ac.za",
